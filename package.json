{
  "name": "frontend-container-test",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.16.0"
  },
  "packageManager": "pnpm@8.5.1",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --dir src",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "lint:style": "stylelint 'src/**/*.{css,scss,sass}'",
    "lint:style:fix": "stylelint --fix 'src/**/*.{css,scss,sass}'",
    "format": "prettier --write --ignore-path .gitignore './src/**/*.{js,jsx,ts,tsx,json}'",
    "unit": "jest",
    "test": "pnpm typecheck && pnpm unit",
    "typecheck": "tsc --noEmit",
    "test:watch": "jest --watch",
    "test:watchAll": "jest --watchAll",
    "storybook": "start-storybook -p 6006 --no-open",
    "build-storybook": "build-storybook -s public",
    "prepare": "husky install"
